import numpy as np
from random import randint

macierz1 = []
macierz1 = np.random.random((8,8))
print("Macierz 1: ")
print(macierz1)

print("\n\n")

'losowanie dla drugiej macierzy'
macierz2 = []
macierz2 = np.random.random((8,8))
print("Macierz 2 : ")
print(macierz2)

print("\n\n")

'Mnozenie macierzy'
wynik=[] #pusta macierz
wynik=macierz1.dot(macierz2)
print("Mno≈ºenie macierzy: ")
print(wynik)
